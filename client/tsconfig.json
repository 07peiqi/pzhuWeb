{
  "compilerOptions": {
    "outDir": "./dis",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "types": ["node_modules/@types","typings/*"],
    "paths":{
      "*":["typs/*"]
    }
  },
  "include": ["src/**/*", "build"],
  "exclude": ["node_modules", "**/*.spec.ts", "dist"]
}
